<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gc.dto.UserDto" table="customer"
		schema="coffeecustomer">


		<id name="id">
			
			<column name="id" sql-type="int(10) unsigned"></column>
		</id>

		<property name="fullname">
			<column name="fullname" sql-type="varchar(255)" length="255"></column>
		</property>
		<property name="emailaddress">
			<column name="emailaddress" sql-type="varchar(255)" length="255"></column>
		</property>
		<property name="pw_hash">
			<column name="password" sql-type="varchar(255)" length="255"></column>
		</property>



	</class>
</hibernate-mapping>